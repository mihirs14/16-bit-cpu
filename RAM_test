    .scope S_0x11ae042f0;
T_0 ;
    %wait E_0x11ae04520;
    %load/vec4 v0x11ae14840_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x11ae04630_0;
    %pad/u 10;
    %ix/vec4 4;
    %load/vec4a v0x11ae04580, 4;
    %assign/vec4 v0x11ae14790_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x11ae14790_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x11ae04170;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x11ae14a10_0;
    %inv;
    %store/vec4 v0x11ae14a10_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x11ae04170;
T_2 ;
